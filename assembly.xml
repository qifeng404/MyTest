<?xml version="1.0" encoding="GBK"?>
<assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 
    <id>tgz</id>
    <!-- 应用名.war（压缩包解压后的目录名） -->
    <baseDirectory>MyTest.war</baseDirectory>
    <formats>
        <!-- 压缩包的格式,一律为tar.gz -->
        <format>tar.gz</format>
    </formats>
 
    <fileSets>
        <fileSet>
            <!-- 要压缩的目录，请按实际目录填写 -->
            <directory>target/MyTest</directory>
            <!-- 输出的目录,此处为空即可 -->
            <outputDirectory></outputDirectory>
            <includes>
                <include>**/**</include>
            </includes>
        </fileSet>
    </fileSets>
</assembly>